<!-- StudyId,SiteCode,ChecklistID,ItemDescription,CompletionStatus,CompletionDate,ResponsiblePerson,Comments -->
<c:FragmentDefinition xmlns:c="sap.ui.core" xmlns="sap.m" xmlns:smfo="sap.ui.comp.smartform" xmlns:smfi="sap.ui.comp.smartfield">
<Dialog >
<smfo:SmartForm editable="true">
	<smfo:layout>
				<smfo:ColumnLayout columnsM="2" columnsL="3" columnsXL="4"/>
			</smfo:layout>
<smfo:Group >
<smfo:GroupElement >
<smfi:SmartField value="{StudyId}" editable="false"/>
</smfo:GroupElement>
<smfo:GroupElement >
<smfi:SmartField value="{SiteCode}" editable="false"/>
</smfo:GroupElement>
<smfo:GroupElement >
<smfi:SmartField value="{ChecklistID}"/>
</smfo:GroupElement>
</smfo:Group>
<smfo:Group >
<smfo:GroupElement >
<smfi:SmartField value="{ItemDescription}"/>
</smfo:GroupElement>
<smfo:GroupElement >
<smfi:SmartField value="{CompletionStatus}"/>
</smfo:GroupElement>
<smfo:GroupElement >
<smfi:SmartField value="{CompletionDate}"/>
</smfo:GroupElement>
<smfo:GroupElement >
<smfi:SmartField value="{ResponsiblePerson}"/>
</smfo:GroupElement>
<smfo:GroupElement >
<smfi:SmartField value="{Comments}"/>
</smfo:GroupElement>
</smfo:Group>
</smfo:SmartForm>
<Toolbar >
<ToolbarSpacer ></ToolbarSpacer>
<Button text="Add" press="onAdd2"/>
<Button text="cancel" press="onClose"/>
</Toolbar>
</Dialog>
</c:FragmentDefinition>